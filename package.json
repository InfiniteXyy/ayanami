{
  "name": "ayanami",
  "version": "0.0.1",
  "description": "A better way to react with state",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeetCode-OpenSource/ayanami.git"
  },
  "bugs": {
    "url": "https://github.com/LeetCode-OpenSource/ayanami/issues"
  },
  "homepage": "https://github.com/LeetCode-OpenSource/ayanami#readme",
  "scripts": {
    "demo": "parcel ./demo/index.html",
    "build": "rm -rf ./dist && tsc -p ./tsconfig.build.json",
    "prettier": "prettier '@(src|demo)/**/*.@(ts|tsx|html|less)' --write",
    "lint": "tslint -p tsconfig.json --fix",
    "test": "jest --collectCoverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint -p tsconfig.json --fix",
      "git add"
    ],
    "*.{less,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "React",
    "hooks",
    "Observables",
    "Observable",
    "model",
    "state",
    "Rx",
    "RxJS",
    "ReactiveX"
  ],
  "author": "LeetCode front-end team",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/shallowequal": "^1.1.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "parcel": "^1.12.3",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.3.4000"
  },
  "peerDependencies": {
    "react": "^16.8.1"
  }
}
